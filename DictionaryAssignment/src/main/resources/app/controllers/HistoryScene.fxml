<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.web.WebView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>

<AnchorPane prefHeight="480.0" prefWidth="800.0" stylesheets="@../css/search.css" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="app.controllers.HistorySceneController">
   <children>
      <AnchorPane prefHeight="80.0" prefWidth="800.0" stylesheets="@../css/search.css">
         <children>
            <TextField fx:id="txtSearch" layoutX="20.0" layoutY="15.0" prefHeight="50.0" prefWidth="760.0" styleClass="search-text" stylesheets="@../css/search.css" />
            <Button fx:id="searchButton" layoutX="735.0" layoutY="20.0" minWidth="40.0" mnemonicParsing="false" prefHeight="40.0" prefWidth="40.0" style="-fx-background-radius: 5px;" styleClass="search-button" stylesheets="@../css/search.css">
               <graphic>
                  <ImageView fitHeight="35.0" fitWidth="35.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../img/search.png" />
                     </image>
                  </ImageView>
               </graphic></Button>
         </children>
      </AnchorPane>
      <ListView fx:id="SearchListView" layoutX="20.0" layoutY="90.0" prefHeight="370.0" prefWidth="180.0" stylesheets="@../css/search.css" />
      <WebView fx:id="webView" layoutX="419.0" layoutY="90.0" prefHeight="370.0" prefWidth="360.0" stylesheets="@../css/search.css" />
      <ListView fx:id="SynonymListView" layoutX="220.0" layoutY="90.0" prefHeight="370.0" prefWidth="180.0" stylesheets="@../css/search.css" />
   </children>
</AnchorPane>

